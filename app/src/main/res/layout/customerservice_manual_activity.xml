<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="24"
    android:orientation="vertical"
    android:background="#ffffff"
    android:clickable="true"
    android:focusableInTouchMode="true"
    >

    <LinearLayout
        android:id="@+id/claim_toolbar"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.8"
        android:gravity="center"
        android:background="#f5f5f5"
        >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            >

            <LinearLayout
                android:id="@+id/CustomerService_Manual_PreBtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                >

                <ImageView
                    android:layout_width="@dimen/Toolbar_Pre_Img"
                    android:layout_height="@dimen/Toolbar_Pre_Img"
                    android:tint="@color/textColor"
                    android:src="@android:drawable/ic_menu_revert"
                    android:textSize="@dimen/Toolbar_Pre_Txt"

                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="이전"
                    android:textColor="@color/textColor"
                    android:textStyle="bold"
                    android:textSize="@dimen/Toolbar_Pre_Txt"
                    />

            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Talented 사용하기"
                android:textStyle="bold"
                android:textSize="@dimen/ToolbarTxt"
                android:gravity="center"
                android:layout_centerInParent="true"
                android:textColor="@color/textColor"
                />

        </RelativeLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        >

        <EditText
            android:id="@+id/CustomerService_Manual_EditTxt"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="8.5"
            android:background="@drawable/edittext_background"
            />
        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.2"/>


        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:text="검색"
            android:textSize="@dimen/ListView_Btn"
            android:textColor="@color/textColor"
            android:gravity="center"
            android:background="@drawable/small_button_whitebackground"
            />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_weight="3.5"
        android:orientation="horizontal"
        >

        <TextView
            android:id="@+id/CustomerService_manual_MyProfile"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="My Profile"
            android:layout_margin="2dp"
            android:textSize="@dimen/List_Title"
            android:textColor="@color/textColor"
            android:background="@drawable/manual_background"
            />

        <TextView
            android:id="@+id/CustomerService_manual_MyTalent"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="My Talent"
            android:gravity="center"
            android:textSize="@dimen/List_Title"
            android:layout_margin="2dp"
            android:textColor="@color/textColor"
            android:background="@drawable/manual_background"
            />

        <TextView
            android:id="@+id/CustomerService_manual_TSharing"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="T.Sharing"
            android:gravity="center"
            android:textSize="@dimen/List_Title"
            android:layout_margin="2dp"
            android:textColor="@color/textColor"
            android:background="@drawable/manual_background"
            />

        <TextView
            android:id="@+id/CustomerService_manual_TSearching"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="재능 검색"
            android:gravity="center"
            android:textSize="@dimen/List_Title"
            android:layout_margin="2dp"
            android:textColor="@color/textColor"
            android:background="@drawable/manual_background"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="3.5"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        >

        <TextView
            android:id="@+id/CustomerService_manual_TCondition"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="나의 재능현황"
            android:textSize="@dimen/List_Title"
            android:layout_margin="2dp"
            android:textColor="@color/textColor"
            android:background="@drawable/manual_background"
            />

        <TextView
            android:id="@+id/CustomerService_manual_CustomerService"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="고객센터"
            android:gravity="center"
            android:textSize="@dimen/List_Title"
            android:textColor="@color/textColor"
            android:layout_margin="2dp"
            android:background="@drawable/manual_background"
            />

        <TextView
            android:id="@+id/CustomerService_manual_System"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="설 정"
            android:gravity="center"
            android:textSize="@dimen/List_Title"
            android:textColor="@color/textColor"
            android:layout_margin="2dp"
            android:background="@drawable/manual_background"
            />

        <TextView
            android:id="@+id/CustomerService_manual_Message"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="Message"
            android:gravity="center"
            android:textSize="@dimen/List_Title"
            android:textColor="@color/textColor"
            android:layout_margin="2dp"
            android:background="@drawable/manual_background"
            />

    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2"
        android:gravity="center"
        android:textSize="@dimen/Page_Txt"
        android:textColor="@color/textColor"
        android:text="문의 내용이 목록에 없는 경우 1:1 문의하기를 활용해주세요."
        />

    <Button
        android:id="@+id/CustomerService_manual_onebyoneQuestionBtn"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:layout_marginTop="5dp"
        android:layout_marginRight="70dp"
        android:layout_marginLeft="70dp"
        android:text="1:1 문의하기"
        android:textSize="@dimen/BigBtn"
        android:textColor="@color/textColor"
        android:background="@drawable/small_button_whitebackground"
        />

</LinearLayout>