<?xml version="1.0" encoding="utf-8"?>

<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawerLayout_CustomerService"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:mContext="com.accepted.acceptedtalentplanet.CustomerService.MainActivity"
    android:background="#ffffff"
    android:orientation="vertical">


    <android.support.v7.widget.Toolbar
        android:id="@+id/customerservice_toolbar"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.8"
        android:contentInsetStart="0dp"
        android:contentInsetLeft="0dp"
        app:contentInsetLeft="0dp"
        app:contentInsetStart="0dp"
        android:padding="0dp"
        android:fitsSystemWindows="true"
        >
        <include
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            layout="@layout/toolbarlayout"/>
    </android.support.v7.widget.Toolbar>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.08"
        android:background="@drawable/brg_divider"
        android:gravity="center_vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:text="서비스 정보"
            android:textSize="@dimen/Divider"
            android:textColor="@color/textcolor_devider"
            android:textStyle="bold"
            />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_Introduction_CustomerService"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2.02"
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="2dp"
            android:paddingLeft="20dp"
            android:paddingTop="2dp"
            android:text="TalentPlanet 소개"
            android:textColor="@color/textColor_object"
            android:textSize="@dimen/DrawerLL_Title"
            android:textStyle="bold"
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.05"
        android:background="#eeeeee" />


    <LinearLayout
        android:id="@+id/ll_Version_CustomerService"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2.02"
        android:background="#ffffff"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingBottom="2dp"
            android:paddingLeft="20dp"
            android:paddingTop="2dp"
            android:text="버전 정보"
            android:textColor="@color/textColor_object"
            android:textSize="@dimen/DrawerLL_Title"
            android:textStyle="bold"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:paddingRight="10dp"
            android:layout_gravity="center_vertical"
            android:textSize="@dimen/List_SubTitle"
            android:textColor="@color/textColor_normal"
            android:text="1.01" />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.05"
        android:background="#eeeeee" />

    <LinearLayout
        android:id="@+id/ll_Update_CustomerService"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2.02"
        android:background="#ffffff">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingBottom="2dp"
            android:paddingLeft="20dp"
            android:paddingTop="2dp"
            android:text="업데이트 내역"
            android:textColor="@color/textColor_object"
            android:textSize="@dimen/DrawerLL_Title"
            android:textStyle="bold"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="right"
            android:paddingRight="10dp"
            android:text="Mar.18"
            android:textColor="@color/textColor_normal"
            android:textSize="@dimen/List_SubTitle"
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.05"
        android:background="#eeeeee" />
    <LinearLayout
        android:id="@+id/ll_Notice_CustomerService"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2.02"
        android:background="#ffffff">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingBottom="2dp"
            android:paddingLeft="20dp"
            android:paddingTop="2dp"
            android:text="공지 사항"
            android:textColor="@color/textColor_object"
            android:textSize="@dimen/DrawerLL_Title"
            android:textStyle="bold"
            />


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.05"
        android:background="#eeeeee" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.08"
        android:background="@drawable/brg_divider"
        android:gravity="center_vertical"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:textStyle="bold"
            android:textColor="@color/textcolor_devider"
            android:textSize="@dimen/Divider"
            android:text="도움말" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_Manual_CustomerService"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2.02"
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="2dp"
            android:paddingLeft="20dp"
            android:paddingTop="2dp"
            android:text="Talented 사용하기"
            android:textColor="@color/textColor_object"
            android:textSize="@dimen/DrawerLL_Title"
            android:textStyle="bold"
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.05"
        android:background="#eeeeee" />

    <LinearLayout
        android:id="@+id/ll_Faq_CustomerService"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2.02"
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="2dp"
            android:paddingLeft="20dp"
            android:paddingTop="2dp"
            android:text="자주하는 질문"
            android:textColor="@color/textColor_object"
            android:textSize="@dimen/DrawerLL_Title"
            android:textStyle="bold"
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.05"
        android:background="#eeeeee" />


    <LinearLayout
        android:id="@+id/ll_Question_CustomerService"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2.02"
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="2dp"
            android:paddingLeft="20dp"
            android:paddingTop="2dp"
            android:text="1:1 문의하기"
            android:textColor="@color/textColor_object"
            android:textSize="@dimen/DrawerLL_Title"
            android:textStyle="bold"
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.05"
        android:background="#eeeeee" />

    <LinearLayout
        android:id="@+id/ll_QuestionList_CustomerService"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2.02"
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="2dp"
            android:paddingLeft="20dp"
            android:paddingTop="2dp"
            android:text="나의 문의내역"
            android:textColor="@color/textColor_object"
            android:textSize="@dimen/DrawerLL_Title"
            android:textStyle="bold"
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.05"
        android:background="#eeeeee" />

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.08"
        android:background="@drawable/brg_divider"
        android:gravity="center_vertical"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:textSize="@dimen/Divider"
            android:textStyle="bold"
            android:textColor="@color/textcolor_devider"
            android:text="신고하기" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_Claim_CustomerService"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2.02"
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="2dp"
            android:paddingLeft="20dp"
            android:paddingTop="2dp"
            android:text="상대방 신고하기"
            android:textColor="@color/textColor_object"
            android:textSize="@dimen/DrawerLL_Title"
            android:textStyle="bold"
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.05"
        android:background="#eeeeee" />

    <LinearLayout
        android:id="@+id/ll_ClaimList_CustomerService"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2.02"
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="2dp"
            android:paddingLeft="20dp"
            android:paddingTop="2dp"
            android:text="신고내역"
            android:textColor="@color/textColor_object"
            android:textSize="@dimen/DrawerLL_Title"
            android:textStyle="bold"
            />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.05"
        android:background="#eeeeee" />


</LinearLayout>


    <FrameLayout
        android:id="@+id/v_drawerView_CustomerService"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start">

        <include
            layout="@layout/drawerlayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </FrameLayout>
</android.support.v4.widget.DrawerLayout>